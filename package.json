{
  "name": "pure-gen",
  "description": "Generate fake data to be used in test automation",
  "version": "0.0.1",
  "contributors": [
    "Armindo Junior <armindo.junior05@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "lint": "npx eslint ./lib",
    "test": "npx mocha test/*.*.js",
    "doc": "npx jsdoc -c conf.json -t ./node_modules/ink-docstrap/template --readme ./README.md",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "all": false,
    "include": [
      "lib/**/*.js",
      "vendor/*.js"
    ],
    "exclude": [
      "coverage",
      "locales",
      "modules",
      "white",
      "docs",
      "docker",
      "public",
      "reports",
      "tests",
      "node_modules"
    ],
    "reporter": [
      "html",
      "lcov",
      "clover"
    ],
    "report-dir": "./reports/coverage"
  },
  "devDependencies": {
    "browserify": "5.11.1",
    "coveralls": "^2.13.1",
    "eslint": "^6.8.0",
    "gulp": "^3.9.1",
    "gulp-gh-pages": "^0.5.4",
    "gulp-jsdoc3": "^0.2.1",
    "gulp-mustache": "0.4.0",
    "gulp-rename": "1.2.0",
    "gulp-uglify": "1.0.1",
    "husky": "^4.2.3",
    "ink-docstrap": "1.1.4",
    "jsdoc": "^3.4.0",
    "lodash": "^4.6.1",
    "mocha": "^3.2.0",
    "node-minify": "*",
    "optimist": "0.3.5",
    "sinon": "1.4.2",
    "slugify": "^1.4.0",
    "through2": "2.0.0",
    "vinyl-transform": "0.0.1"
  },
  "files": [
    "lib",
    "build/build/faker.js",
    "build/build/faker.min.js",
    "locale",
    "!locale/.publish",
    "vendor"
  ],
  "license": "MIT",
  "main": "index.js",
  "dependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "",
      "pre-push": ""
    }
  }
}
